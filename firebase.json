{
  "hosting": {
    "public": "public/_site",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects":[ 
      {
        "source": "/cypress-3",
        "destination": "/cypress-3.html",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
